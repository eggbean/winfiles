# ssh_config for Windows

Match host * exec "arp -a 192.168.88.1 | findstr c4-ad-34-bd-72-83 >NUL"
	ProxyJump none

CanonicalizeHostname yes
CanonicalDomains jinkosystems.co.uk
CanonicalizeMaxDots 3
CanonicalizeFallbackLocal yes

Match all
	Include ~/.ssh/*.conf

Host *.jinkosystems.co.uk
	ProxyJump bastet
	ForwardAgent yes
	StrictHostKeyChecking yes
	RemoteForward /c/Users/jason/AppData/Local/gnupg/S.gpg-agent /c/Users/jason/AppData/Local/gnupg/S.gpg-agent.extra

Host bastet baal bastion
	ProxyJump none
	StrictHostKeyChecking yes

Host mikrotik
	HostName 192.168.88.1
	ProxyJump none

Host paradroid
	User u0_a423
	Port 8022
	ProxyJump none

Host *
	User jason
	IdentityFile ~/.ssh/id_ed25519.pub
	IdentitiesOnly yes
	AddKeysToAgent yes
	ForwardAgent no
	GSSAPIAuthentication no
	StrictHostKeyChecking accept-new
	HashKnownHosts no
	ServerAliveInterval 5
	ServerAliveCountMax 1
